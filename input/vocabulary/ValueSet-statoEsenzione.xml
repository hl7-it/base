<?xml version="1.0" encoding="UTF-8"?>
<ValueSet xmlns="http://hl7.org/fhir"
        	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        	xsi:schemaLocation="http://hl7.org/fhir ../../../fhir-all-xsd/structuredefinition.xsd">
	<id value="statoEsenzione"/>
	<text xmlns:xhtml="http://www.w3.org/1999/xhtml">
		<status value="additional"/>
		<xhtml:div>
			<xhtml:h4>Interpretazione dei codici di ActStatus per le esenzioni</xhtml:h4>
			<xhtml:table>
				<xhtml:thead>
					<xhtml:tr>
						<xhtml:th>Codice</xhtml:th>
						<xhtml:th>Uso</xhtml:th>
					</xhtml:tr>
				</xhtml:thead>
				<xhtml:tbody>
					<xhtml:tr>
						<xhtml:td>active</xhtml:td>
						<xhtml:td>Esenzione in corso di validità</xhtml:td>
					</xhtml:tr>
					<xhtml:tr>
						<xhtml:td>suspended</xhtml:td>
						<xhtml:td>Esenzione momentaneamente sospesa (ad esempio in attesa del rinnovo di un’iscrizione temporanea)</xhtml:td>
					</xhtml:tr>
					<xhtml:tr>
						<xhtml:td>aborted</xhtml:td>
						<xhtml:td>Esenzione mai stata valida (ad esempio è stata assegnata per errore e il documento corrispondente era già stato prodotto in stato active)</xhtml:td>
					</xhtml:tr>
					<xhtml:tr>
						<xhtml:td>completed</xhtml:td>
						<xhtml:td>Esenzione non più in corso di validità</xhtml:td>
					</xhtml:tr>
					<xhtml:tr>
						<xhtml:td>UNK</xhtml:td>
						<xhtml:td>Lo stato dell'esenzione non è noto</xhtml:td>
					</xhtml:tr>
				</xhtml:tbody>
			</xhtml:table>
		</xhtml:div>
	</text>
	<url value="http://hl7.it/fhir/ValueSet/statoEsenzione"/>
	<version value="0.1.0"/>
	<name value="VsStatoEsenzione"/>
	<title value="Stato Esenzione"/>
	<status value="active"/>
	<description value="Value Set basato sul vocabolario HL7 V3 ActStatus che descrive lo stato delle esenzioni. Questo value set &#232; adottato da HL7 CDA R2 IG &apos;DOCUMENTO DI ESENZIONE&apos;"/>
	<compose>
		<include>
			<system value="http://terminology.hl7.org/CodeSystem/v3-ActStatus"/>
			<concept>
				<extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
					<valueString value="Esenzione in corso di validità"/>
				</extension>
				<code value="active"/>
				<display value="attivo"/>
			</concept>
			<concept>
				<extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
					<valueString value="Esenzione momentaneamente sospesa (ad esempio in attesa del rinnovo di un’iscrizione temporanea)"/>
				</extension>
				<code value="suspended"/>
				<display value="sospeso"/>
			</concept>
			<concept>
				<extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
					<valueString value="Esenzione mai stata valida (ad esempio è stata assegnata per errore e il documento corrispondente era già stato prodotto in stato active)"/>
				</extension>
				<code value="aborted"/>
				<display value="abortito"/>
			</concept>
			<concept>
				<extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
					<valueString value="Esenzione non più in corso di validità"/>
				</extension>
				<code value="completed"/>
				<display value="completato"/>
			</concept>
		</include>
		<include>
			<system value="http://terminology.hl7.org/CodeSystem/v3-NullFlavor"/>
			<concept>
				<extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
					<valueString value="Lo stato dell'esenzione non è noto"/>
				</extension>
				<code value="UNK"/>
				<display value="sconosciuto"/>
			</concept>
		</include>
	</compose>
</ValueSet>
